<Window x:Class="Xe.Tools.Components.KernelEditor.Views.DialogElementsSelection"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Xe.Tools.Components.KernelEditor.Views"
        mc:Ignorable="d"
        Title="Selection" Height="Auto" Width="440" SizeToContent="Height">
    <DockPanel Margin="10">
        <StackPanel Margin="0 10 0 0" DockPanel.Dock="Bottom" Orientation="Horizontal" HorizontalAlignment="Right">
            <Button Margin="10 0 0 0" Content="_Cancel" IsDefault="True" Width="75" IsCancel="True"/>
            <Button Margin="10 0 0 0" Content="_Ok" IsDefault="True" Width="75" Click="Button_Click"/>
        </StackPanel>
        <local:BitmaskSelection/>
    </DockPanel>
</Window>
